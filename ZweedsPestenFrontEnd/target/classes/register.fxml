<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="255.0" prefWidth="225.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="frontend.controllers.RegisterController">
   <children>
      <Label layoutX="38.0" layoutY="34.0" text="Gebruikersnaam" />
      <Label layoutX="38.0" layoutY="83.0" text="Wachtwoord" />
      <Label layoutX="38.0" layoutY="131.0" text="Herhaal wachtwoord" />
      <TextField fx:id="tfUsername" layoutX="38.0" layoutY="51.0" />
      <PasswordField fx:id="pfPassword" layoutX="38.0" layoutY="100.0" />
      <PasswordField fx:id="pfPasswordConfirmation" layoutX="38.0" layoutY="148.0" />
      <Button fx:id="btnRegister" layoutX="117.0" layoutY="200.0" mnemonicParsing="false" onAction="#processBtnRegisterClicked" text="Registreer">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="btnBack" layoutX="34.0" layoutY="200.0" mnemonicParsing="false" onAction="#processBtnBackClicked" text="&lt; Terug">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
